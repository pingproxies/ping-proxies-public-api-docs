{
  "openapi": "3.0.1",
  "info": {
    "title": "Proxy Tester Routes",
    "version": "1.0",
    "description": "Proxy tester specific routes extracted from the main API"
  },
  "paths": {
    "/public/proxy_tester/proxy_test_run/create": {
      "post": {
        "description": "Test all provided proxies against urls",
        "tags": [
          "Proxy Tester"
        ],
        "summary": "Create Proxy Test",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ProxyTestRunCreateRequest"
              }
            }
          },
          "description": "Test Run Request",
          "required": true
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProxyTestRunCreateResponse"
                }
              }
            },
            "description": "Successful proxy test creation"
          }
        },
        "security": [
          {
            "APIPrivateKey": [],
            "APIPublicKey": []
          }
        ]
      }
    },
    "/public/proxy_tester/proxy_test_server/search": {
      "get": {
        "tags": [
          "Proxy Tester"
        ],
        "summary": "Search active proxy test servers",
        "description": "Search the currently active proxy test servers",
        "operationId": "search-proxy-test-servers",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProxyTestServerSearchResponse"
                }
              }
            }
          },
          "404": {
            "description": "Not Found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        },
        "security": [
          {
            "APIPrivateKey": [],
            "APIPublicKey": []
          }
        ]
      }
    },
    "/public/proxy_tester/proxy_test_run/retrieve/{test_run_id}": {
      "get": {
        "tags": [
          "Proxy Tester"
        ],
        "summary": "Retrieve proxy test results",
        "description": "Retrieve the results from the given proxy test run",
        "operationId": "retrieve-proxy-test",
        "parameters": [
          {
            "name": "test_run_id",
            "in": "path",
            "description": "Test Run ID",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "proxy_test_run_password",
            "in": "query",
            "description": "Retrieve test run with password",
            "required": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProxyTestRunCreateResponse"
                }
              }
            }
          },
          "404": {
            "description": "Not Found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        },
        "security": [
          {
            "APIPrivateKey": [],
            "APIPublicKey": []
          }
        ]
      }
    }
  },
  "components": {
    "schemas": {
      "ProxyTestRunCreateRequest": {
        "type": "object",
        "properties": {
          "proxies": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "proxy_id": {
                  "type": "string"
                },
                "proxy_string": {
                  "type": "string"
                }
              }
            },
            "example": [
              {
                "proxy_id": "8ca1bf7f-7e8e-490e-ad0a-715e5eb060a6",
                "proxy_string": "209.99.134.6:5702:ftrdddgq:518vrc41xyms"
              }
            ]
          },
          "proxy_tester_server_id": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "example": [
              "london-2-digitalocean-proxytester-1"
            ]
          },
          "proxy_test_run_id": {
            "type": "string",
            "format": "uuid",
            "example": "8ca1bf7f-7e8e-490e-ad0a-715e5eb060a6"
          },
          "urls": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "example": [
              "https://google.com"
            ]
          }
        }
